{
    "name": "polymorphine/http",
    "description": "PHP Front Controller for HTTP applications",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "polymorphine/container": "@dev",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "polymorphine/cs": "@dev",
        "phpunit/phpunit": "^6.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shudd3r",
            "email": "q3.shudder@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Polymorphine\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polymorphine\\Http\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "run-tests": "phpunit --configuration phpunit.xml.dist"
    }
}
